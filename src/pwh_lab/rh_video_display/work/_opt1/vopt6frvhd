library verilog;
use verilog.vl_types.all;
entity musical_score_loader is
    port(
        clk             : in     vl_logic;
        reset           : in     vl_logic;
        song_id         : in     vl_logic;
        next_notes_out  : out    vl_logic_vector(63 downto 0)
    );
end musical_score_loader;
